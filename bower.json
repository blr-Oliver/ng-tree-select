{
  "name": "ng-tree-select",
  "description": "Recursive angular.js tree select component",
  "main": "ng-tree-select.js",
  "authors": [
    "Vasily Liaskovsky <vasily.liaskovsky@gmail.com>"
  ],
  "license": "MIT",
  "keywords": [
    "angular",
    "tree",
    "recursive",
    "select"
  ],
  "dependencies": {
    "angular": "^1.6.4",
    "angular-recursion": "^1.0.5",
    "ng-css-injector": "^1.0.1",
    "ng-slide-animations": "^1.1.1"
  },
  "homepage": "https://github.com/blr-Oliver/ng-tree-select",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "**/test",
    "tests",
    "tmp"
  ],
  "install": {
    "options": {
      "includePackageNameInInstallPath": false
    },
    "path": "test/lib",
    "sources": {
      "angular": "bower_components/angular/angular.js",
      "angular-recursion": "bower_components/angular-recursion/angular-recursion.js",
      "ng-css-injector": "bower_components/ng-css-injector/ng-css-injector.js",
      "ng-slide-animations": "bower_components/ng-slide-animations/src/ng-slide-animations.js"
    }
  }
}
